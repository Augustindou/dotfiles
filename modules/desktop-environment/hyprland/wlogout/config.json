[
    {
        "label": "lock",
        "action": "hyprlock",
        "text": "Lock",
        "keybind": "l"
    },
    {
        "label": "reboot",
        "action": "systemctl reboot",
        "text": "Reboot",
        "keybind": "r"
    },
    {
        "label": "shutdown",
        "action": "systemctl poweroff",
        "text": "Shutdown",
        "keybind": "s"
    },
    {
        "label": "suspend",
        "action": "hyprlock & systemctl suspend",
        "text": "Suspend",
        "keybind": "u"
    }
]